        function getASPElm(nm, tag) {
            if ($get(nm)) { return $get(nm); }
            if (!tag) { tag = '*'; }
            var e = document.getElementsByTagName(tag);
            for (var i = 0; i < e.length; i++) {
                if (e[i].id) {
                    if (e[i].id.indexOf(nm) != -1) {
                        return e[i];
                    }
                }
            }
            return null;
        }
